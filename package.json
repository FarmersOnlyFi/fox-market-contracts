{
  "name": "fox-market-contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "-------BASE--------": "",
    "compile": "npx hardhat compile",
    "------DEPLOY------": "",
    "deploy:klaytn": "npx hardhat --network klaytn deploy",
    "deploy:dfk": "npx hardhat --network dfk deploy",
    "-----UPGRADE-----": "",
    "upgrade:klaytn": "npx hardhat --network klaytn upgrade",
    "upgrade:dfk": "npx hardhat --network dfk upgrade",
    "-------KLAYTN------": "",
    "verify:klaytn": "npx hardhat --network klaytn verify",
    "verify:dfk": "npx hardhat --network dfk verify",
    "----KLAYTN----": "",
    "foxMarket:klaytn:deploy": "npm run deploy:klaytn -- --name FoxMarket",
    "foxMarket:klaytn:upgrade": "npm run upgrade:klaytn -- --name FoxMarket",
    "foxMarket:klaytn:verify": "npm run verify:klaytn -- --name FoxMarket",
    "-------DFK--------": "",
    "foxMarket:dfk:deploy": "npm run deploy:dfk -- --name FoxMarket",
    "foxMarket:dfk:upgrade": "npm run upgrade:dfk -- --name FoxMarket",
    "foxMarket:dfk:verify": "npm run verify:dfk -- --name FoxMarket"
  },
  "author": "dirtycajunrice",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "4.8.0",
    "@openzeppelin/upgrades-core": "1.20.5",
    "@poanet/solidity-flattener": "^3.0.8",
    "dotenv": "16.0.3",
    "hardhat": "2.12.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@chainlink/contracts": "0.5.1",
    "@nomicfoundation/hardhat-toolbox": "2.0.0",
    "@openzeppelin/contracts-upgradeable": "4.8.0",
    "@openzeppelin/hardhat-upgrades": "1.21.0"
  }
}
